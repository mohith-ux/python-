name="mohith"
b=name.upper()
c=name.lower()
d=name.capitalize()
e=name.swapcase()
print(b)
print(c)
print(d)
print(e)

#string cheking method
str="abcd"
s1="1234a"
s2="1123"
s3="Coffin"
s4="Coffin"
s5="   "
s6="The Animal"
print(str.isalpha())
print(s1.isdigit())
print(s2.isalnum())
print(s3.islower())
print(s4.isupper())
print(s5.isspace())
print(s6.istitle())
